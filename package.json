{
    "name": "sails-react-bootstrap-webpack",
    "version": "1.9.0",
    "description": "An opinionated base configuration of Sails, with Webpack for React support, and Bootstrap for styling.",
    "keywords": [
        "sails",
        "react",
        "bootstrap",
        "webpack",
        "mocha",
        "chai",
        "react-bootstrap"
    ],
    "dependencies": {
        "bootstrap": "^4.5.2",
        "copy-webpack-plugin": "^6.0.3",
        "csrf": "^3.1.0",
        "json-stringify-safe": "^5.0.1",
        "lodash": "^4.17.20",
        "md5": "^2.3.0",
        "moment-timezone": "^0.5.31",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-bootstrap": "^1.3.0",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "react-transition-group": "^4.4.1",
        "sails": "^1.2.5",
        "sails-hook-orm": "^3.0.1",
        "sails-hook-sockets": "^2.0.0",
        "sails-mysql": "^1.0.1",
        "scrypt-kdf": "^2.0.1",
        "serve-static": "^1.14.1",
        "superagent": "^6.0.0",
        "uuid": "^8.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.11.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "chai": "^4.2.0",
        "chai-spies": "^1.0.0",
        "chai-uuid": "^1.0.6",
        "core-js": "^3.6.5",
        "css-loader": "^4.2.2",
        "eslint": "^7.7.0",
        "eslint-plugin-react": "^7.20.6",
        "favicons-webpack-plugin": "^4.2.0",
        "file-loader": "^6.0.0",
        "fixted": "^3.0.1",
        "html-webpack-plugin": "^4.3.0",
        "mini-css-extract-plugin": "^0.10.0",
        "mocha": "^8.1.1",
        "node-sass": "^4.14.1",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.2",
        "sass-loader": "^9.0.3",
        "style-loader": "^1.2.1",
        "supertest": "^4.0.2",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.1.2"
    },
    "scripts": {
        "coverage": "NODE_ENV=testing nyc --reporter=html mocha",
        "test": "npm run lint && npm run custom-tests && echo 'Done.'",
        "lint": "./node_modules/eslint/bin/eslint.js . --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'",
        "custom-tests": "NODE_ENV=testing mocha",
        "start": "run-p open:client lift",
        "start:debug": "npm-run-all --parallel open:client debug",
        "open:client": "webpack-dev-server --mode development --open --config webpack/dev.config.js",
        "build": "run-s clean build:prod",
        "build:dev": "run-s clean build:dev:webpack",
        "build:dev:webpack": "webpack --mode development --config webpack/dev-build.config.js",
        "build:prod": "NODE_ENV=production webpack --mode production --config webpack/prod.config.js",
        "clean": "rimraf .tmp && mkdirp .tmp/public",
        "lift": "node app.js",
        "debug": "node --inspect app.js",
        "lines": "git ls-files --exclude-standard -- ':!:**/*.[pjs][npv]g' ':!:**/*.ai' ':!:.idea' ':!:test/coverage/*' ':!:config/*' ':!:.babelrc' ':!:.editorconfig' ':!:*.eslintrc' ':!:.eslintignore' ':!:.gitignore' ':!:.mocharc.yaml' ':!:.npmrc' ':!:.nycrc' ':!:.sailsrc' ':!:LICENSE' ':!:package-lock.json' | xargs wc -l"
    },
    "main": "app.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/neonexus/sails-react-bootstrap-webpack.git"
    },
    "author": "NeoNexus DeMortis",
    "license": "Unlicense",
    "engines": {
        "node": "^10.15"
    }
}
